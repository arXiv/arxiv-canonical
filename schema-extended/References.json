{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "References",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "references": {
      "title": "List of references",
      "description": "List of unstructured reference strings.",
      "type": "array",
      "minItems": 1,
      "items": {
        "reference_string": {
          "title": "Reference string",
          "description": "An unstructured reference string.",
          "type": "string",
          "minLength": 1
        }
      },
      "uniqueItems": true
    }
  }
}
